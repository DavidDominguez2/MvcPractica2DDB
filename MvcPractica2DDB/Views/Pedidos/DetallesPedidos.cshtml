@model List<VistaPedidos>

    <h1>Resumen Pedidos</h1>

<table class="table table-dark">
    <thead>
        <tr>
            <th>Fecha</th>
            <th>Titulo</th>
            <th>Precio</th>
            <th>Precio final</th>
        </tr>

    </thead>
    <tbody>
        @foreach(VistaPedidos pedidos in Model) {
            <tr>
                <td>@pedidos.Fecha</td>
                <td>@pedidos.Titulo</td>
                <td>@pedidos.Precio</td>
                <td>@pedidos.PrecioFinal</td>
            </tr>
        }
    </tbody>
</table>